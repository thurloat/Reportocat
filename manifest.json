{
	"name": "Reportocat",
	"version": "0.1",
	"description": "Quickly Log Github Issues",
	"background_page": "background.html",
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Reportocat",
		"default_popup": "popup.html"
	},
	"permissions": [
      "contextMenus",
      "notifications",
      "tabs",
      "http://github.com/",
      "https://github.com/",
      "https://api.github.com/",
      "http://api.github.com/"
    ]
}
